{
	"name": "@nextnode/nextnode-front",
	"version": "0.0.0",
	"description": "Nextnode company homepage developed with Astro",
	"keywords": [
		"astro",
		"frontend",
		"webapp"
	],
	"license": "MIT",
	"author": "Nextnode",
	"scripts": {
		"build": "astro check && astro build",
		"clean": "rm -rf node_modules/.vite .astro dist",
		"commitlint": "commitlint --edit",
		"dev": "astro dev",
		"format": "prettier --write .",
		"lint": "biome check --write --no-errors-on-unmatched",
		"lint-staged": "lint-staged",
		"prepare": "husky",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"type-check": "astro check && tsc --noEmit",
		"preview": "astro preview"
	},
	"dependencies": {
		"@astrojs/node": "^9.4.2",
		"@astrojs/react": "4.3.0",
		"@astrojs/sitemap": "^3.6.0",
		"@calcom/embed-react": "^1.5.3",
		"@nextnode/config-manager": "3.1.0",
		"@nextnode/logger": "^0.2.3",
		"@radix-ui/react-accordion": "^1.2.11",
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-progress": "^1.1.7",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-slot": "^1.0.2",
		"@react-email/components": "^0.5.0",
		"@tailwindcss/vite": "^4.1.11",
		"astro": "^5.13.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"i18next": "^25.3.2",
		"lucide-react": "^0.487.0",
		"motion": "^12.23.24",
		"prismjs": "^1.30.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-email": "^4.2.8",
		"resend": "^6.0.1",
		"sharp": "^0.33.5",
		"tailwind-merge": "^2.2.0",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@biomejs/biome": "^2.2.3",
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@nextnode/standards": "^2.0.5",
		"@playwright/test": "^1.54.2",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.1.0",
		"@types/prismjs": "^1.26.5",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitest/coverage-v8": "^3.1.4",
		"better-sort-package-json": "^1.1.1",
		"husky": "^9.0.11",
		"jsdom": "^26.1.0",
		"lint-staged": "^15.2.2",
		"playwright": "^1.54.2",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"typescript": "^5.0.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.4"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "pnpm@10.12.4",
	"type": "module",
	"prettier": "@nextnode/standards/prettier/astro"
}
